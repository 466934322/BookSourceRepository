{
  "name": "My716",
  "version": 104,
  "category": 1,
  "url": "http://api.zhuishushenqi.com",
  "charset": "utf-8",
  "metadata": {
    "name": ["$.title"],
    "author": ["$.author"],
    "cover": ["$.cover@js->'http://statics.zhuishushenqi.com' + ${this}"],
    "summary": ["$.shortIntro"],
    "category": ["$.cat", "$.majorCate"],
    "update": ["$.[?(@.source=='my176')].updated@js->function getTime(str){ var o = ''; var d = new Date(str); o = d.getUTCFullYear()+'-'+(d.getUTCMonth()+1)+'-'+d.getUTCDate()+' '+d.getUTCHours()+':'+d.getUTCMinutes()+':'+d.getUTCSeconds(); return o; } getTime(${this});"],
    "link": ["$._id@js->'http://api.zhuishushenqi.com/atoc?view=summary&book=' + ${this}"],
    "catalog": ["$.[?(@.source=='my176')]._id@js->'http://api.zhuishushenqi.com/atoc/' + ${this} + '?view=chapters'"]
  },
  "catalog": {
    "list": "$.chapters",
    "orderBy": 0,
    "chapter": {
      "name": "$.title",
      "link": "$.link@js->'http://chapterup.zhuishushenqi.com/chapter/' + encodeURIComponent(${this})"
    }
  },
  "content": {
    "text": "$.*.body"
  },
  "search": {
    "link": "http://api.zhuishushenqi.com/book/fuzzy-search?query=${key}",
    "list": "$.books[?(@.contentType =='txt')]"
  },
