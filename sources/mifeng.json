{
  "name": "mifeng",
  "version": 100,
  "category": 1,
  "url": "http://www.mifengxs.com",
  "charset": "gbk",
  "metadata": {
    "name": ["td:first-child>a", ".book-info>h1"],
    "author": ["td:nth-child(3)>a", ".book-stats>b:first-child"],
    "cover": [".book-img>img@attr->src"],
    "summary": [".book-intro>a"],
    "category": ["#bdshare>a:nth-child(2)"],
    "status": ["td:nth-child(6)>a", ".book-stats>b:nth-child(2)"],
    "update": ["td:nth-child(5)>a", ".book-stats>b:nth-child(2)"],
    "lastChapter": ["td:nth-child(2) > a"],
    "link": ["td:first-child > a@attr->href"],
    "catalog": [".book-link>a@attr->href"]
  },
  "catalog": {
    "list": "#list > dl",
    "orderBy": 0,
    "chapter": {
      "name": "dd>a",
      "link": "dd>a@attr->href"
    }
  },
  "content": {
    "text": "#content"
  },
  "search": {
    "link": "http://www.mifengxs.com/modules/article/search.php@post->searchkey=${key}&searchtype=articlename",
    "list": "tbody > tr"
  }
}